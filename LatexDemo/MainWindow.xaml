<Window x:Class="LatexDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LatexDemo"
        mc:Ignorable="d"
        Title="LaTeX Magic">

    <Grid>
        <InkCanvas x:Name="ink" Height="100" Background="#e1e6ef" HorizontalAlignment="Center" Margin="0,84,0,323.5"/>
        <Button x:Name="btnPredict" Content="Predict" HorizontalAlignment="Center" Margin="332,284,331,0" VerticalAlignment="Top" Width="100"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Center" Height="23" Margin="332,410,331,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100"/>
        <Image x:Name="imageLatex" HorizontalAlignment="Center" Margin="0,84,0,323.5" Visibility="Collapsed"></Image>
    </Grid>

</Window>
